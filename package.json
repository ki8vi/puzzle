{
  "name": "rss-puzzle",
  "main": "webpack.base.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.{ts,css}'",
    "ci:format": "prettier --check '**/*.{ts,css}'",
    "prepare": "cd .. && husky rss-puzzle/.husky",
    "validate-branch": "node validate-branch.js",
    "deploy": "npm run build && npx gh-pages -d dist -e rss-puzzle"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "gh-pages": "^6.1.1",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "mini-css-extract-plugin": "^2.8.1",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  }
}
